{
  "name": "governance-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute governance-db --file migrations/0001_initial.sql",
    "db:migrate:remote": "wrangler d1 execute governance-db --remote --file migrations/0001_initial.sql",
    "db:migrate:local": "wrangler d1 execute governance-db --local --file migrations/0001_initial.sql",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@superbenefit/porch": "workspace:*",
    "agents": "^0.0.85",
    "hono": "^4.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "notes": {
    "noble-hashes": "Install @noble/hashes for keccak256 in ens.ts: npm install @noble/hashes"
  }
}
